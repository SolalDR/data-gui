{
  "entryPoints": ["src/index.ts", "src/extras"],
  "entryPointStrategy": "expand",
  "exclude": [
    "example/*",
    "src/helpers/*"
  ],
  "excludeExternals": true,
  "excludeProtected": true,
  "excludePrivate": true,
  "categorizeByGroup": false,
  "mergeModulesMergeMode": "project",
  "media": "./public",
  "out": "docs",
  "sort": ["source-order"]
}